{
    "expo": {
        "name": "kfiles",
        "slug": "kfiles",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.excel.fyllo"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "android": {
            "package": "com.excel.fyllo",
            "versionCode": 6,
            "permissions": []
        }
    }
}